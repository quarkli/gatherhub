{
  "name": "hubCom Plugin",
  "description": "This extension will capture screen.",
  "version": "0.1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "browser_action": {
        "default_icon": "icon.png"
        },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "js": [ "content.js" ],
    "matches": [
    	"https://192.168.10.10:2013/*",
      "https://52.69.118.100:2013/*"
        ]
  }],
  "externally_connectable": {
      "matches": [
    	"https://192.168.10.10:2013/*",
      "https://52.69.118.100:2013/*"
      ]
  }
}
